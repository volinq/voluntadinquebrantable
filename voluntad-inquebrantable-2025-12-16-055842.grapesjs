ell."}]}]}]}]},{"name":"Programs Grid","classes":["programs-grid"],"attributes":{"id":"i254vj"},"components":[{"tagName":"article","classes":["program-card"],"attributes":{"id":"i6a3y8"},"components":[{"classes":["program-header"],"attributes":{"id":"i40heo"},"components":[{"classes":["program-icon-wrapper"],"attributes":{"id":"i6ja6f"},"components":[{"type":"image","resizable":{"ratioDefault":1},"classes":["program-icon"],"attributes":{"alt":"Parent education icon","src":"https://api.iconify.design/lucide-users.svg?color=%230f766e","id":"ie75he"}}]},{"tagName":"h3","type":"heading","classes":["program-title"],"attributes":{"id":"i30wqm"},"components":[{"type":"textnode","content":"Parent Education"}]}]},{"tagName":"p","type":"text","classes":["program-description"],"attributes":{"id":"inx9xg"},"components":[{"type":"textnode","content":"\n      Workshops on child development, positive discipline, advocacy, and navigating school resources to support learning at home.\n    "}]}]},{"tagName":"article","classes":["program-card"],"attributes":{"id":"i7bfzn"},"components":[{"classes":["program-header"],"attributes":{"id":"i4t2if"},"components":[{"classes":["program-icon-wrapper"],"attributes":{"id":"i4enwb"},"components":[{"type":"image","resizable":{"ratioDefault":1},"classes":["program-icon"],"attributes":{"alt":"Youth education icon","src":"https://api.iconify.design/lucide-graduation-cap.svg?color=%232b6cb0","id":"i4jxoq"}}]},{"tagName":"h3","type":"heading","classes":["program-title"],"attributes":{"id":"ioa3v3"},"components":[{"type":"textnode","content":"Youth Education"}]}]},{"tagName":"p","type":"text","classes":["program-description"],"attributes":{"id":"i2herm"},"components":[{"type":"textnode","content":"\n      Tutoring, homework help, STEM and arts clubs, plus college and career readiness to keep students on track and inspired.\n    "}]}]},{"tagName":"article","classes":["program-card"],"attributes":{"id":"i0nt17"},"components":[{"classes":["program-header"